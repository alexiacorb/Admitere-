I. 1. a) 
#include <iostream>
using namespace std;
int prim(int n){
    if(n==0 || n==1){
        return 0;
    }
    for(int d=2;d*d<=n;d++){
        if(n%d==0){
            return 0;
        }
    }
    return 1;
}
int main() {
    cout<<prim(12)<<endl;
    return 0;
}
b) #include <iostream>

using namespace std;
int prim(int n){
    if(n==0 || n==1){
        return 0;
    }
    for(int d=2;d*d<=n;d++){
        if(n%d==0){
            return 0;
        }
    }
    return 1;
}
void afisare(int n,int x[100]){
    for(int i=1;i<n;i++){
        for(int j=i+1;j<=n;j++){
            if(prim(x[i]+x[j])==1){
                cout<<"("<<i<<","<<j<<")";
            }
        }
    }
}
int main() {
    int n, x[100];
    cin>>n;
    for(int i=1;i<=n;i++){
        cin>>x[i];
    }
    afisare(n,x);
    return 0;
}
