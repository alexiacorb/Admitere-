using namespace std;
int cmmdc(int a,int b){
    if(a==0 || b==0){
        return -1;
    }
    if(a==b){
        return a;
    } 
    if(a>b){
        return cmmdc(a-b,b);
    }
    else if(a<b){
        return cmmdc(a,b-a);
    }
    return 0;
}
int main()
{
    int a,b;
    cin>>a>>b;
    cout<<cmmdc(a,b);
    return 0;
}
