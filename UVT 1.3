5. #include <iostream>

using namespace std;
 
int main()
{
    int m,n,a[100][100],lin,col;
    cin>>m>>n;
    for(int i=1;i<=m;i++){
        for(int j=1;j<=n;j++){
            cin>>a[i][j];       
        }
    }
    cin>>lin>>col;
    for(int i=lin;i<=m;i++){
        for(int j=1;j<=n;j++){
            a[i][j]=a[i+1][j];
        }
    }
    for(int j=col;j<=n;j++){
        for(int i=1;i<=m;i++){
            a[i][j]=a[i][j+1];
        }
    }
    for(int i=1;i<=m-1;i++){
        for(int j=1;j<=n-1;j++){
            cout<<a[i][j]<<" ";       
        }
        cout<<endl;
    }
    return 0;
}
