using namespace std;

int main()
{
    int n,m,a[1000],x,d[1000]={};
    cin>>n>>m;
    if(n==m){
        for(int i=1;i<=n;i++){
            cin>>a[i];
        }
        for(int i=1;i<=n;i++){
            cin>>x;
            d[i]=x+a[i];
        }
    }
    else{
        if(n<m){
            for(int i=1;i<=n;i++){
               cin>>a[i];
            }
            for(int i=1;i<=m;i++){
                cin>>x;
                if(i<=n){
                    d[i]=x+a[i];
                }
                else{
                    d[i]=x;
                }
            }
        }
        else{
            for(int i=1;i<=n;i++){
               cin>>a[i];
               if(i>m){
                   d[i]=a[i];
               }
            }
            for(int i=1;i<=m;i++){
                cin>>x;
                d[i]=x+a[i];
            }
        }
    }
     for(int i=1;i<=max(n,m);i++){
            cout<<d[i]<<" ";
        }
    return 0;
}
