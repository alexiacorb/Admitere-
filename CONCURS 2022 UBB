12. n=10 used[10]=True sum=1*1*1+0*0*0=1 n=sum=1 nr=1
         used[1]=True sum=1*1*1=1 n=1 nr=2
    n=10 -->2 
    n=3 -->used[3]=True sum=27 n=sum=27 nr=1
           used[27]=True sum=7*7*7+2*2*2=8+49*7=351 n=351 nr=2
           used[351]=True sum=1+125+27=125+28=153 n=153 nr=3
           used[153]=True sum=27+1+125=153 n=153 nr=4
    n=3 -->4 
    n=30 -->used[30]=True sum=27 n=27 nr=1
           used[27]=True sum=7*7*7+2*2*2=8+49*7=351 n=351 nr=2
           used[351]=True sum=1+125+27=125+28=153 n=153 nr=3
           used[153]=True sum=27+1+125=153 n=153 nr=4
    n=30 -->4 
    R: ACD
