27. A.(st MOD 10) * putere(10, nrCifre(dr)) + dr 
      		 produs(1092, 0)
			drCrt=1092%10*putere(10,1)+dr=2*10+0=20
			stCrt=109
			1092*0<109*20 A-->produs(109,20)
			 		 produs(109,20)
			drCrt=9*(10,2)+dr=920
			stCrt=10
			109*20<920*10 -->			2180<9200 A--> produs(920,10)
					produs(10,920)
			drCrt=0*(10,3)+920=920
			stCrt=1
			10*920<1*920 f-->produs(1092, 0)=920*10=9200
		B. (st MOD 10) * putere(10, dr) + dr
			  produs(1092, 0)
			drCrt=1092%10*putere(10,0)+dr=2*1+0=2
			stCrt=109
			1092*0<109*2 A--> produs(109,2)
		  	produs(109,2) 
			drCrt=9*100+dr=900+2=902
			stCrt=10
			109*2<10*902 A -->produs(10,902)
			drCrt=
	C. (st MOD 10) * putere(10, nrCifre(dr))
        produs(1092, 0)
			drCrt=1092%10*putere(10,1)=2*10=20
			stCrt=109
			1092*0<109*20 A-->produs(109,20)
			 		 produs(109,20)
			drCrt=9*(10,2)=900
			stCrt=10
			109*20<900*10 -->			2180<9000 A--> produs(900,10)
					produs(10,900)
			drCrt=0*(10,3)=0
			stCrt=1 
