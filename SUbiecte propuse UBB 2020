11. v[1] = 1; v[2] = 4; v[3] = 2;
    v[4] = 3; v[5] = 3; v[6] = 10;
    v[7] = 12;
    
    s=v[2]-v[1]=3
    v[2]==v[1] F
    s=3+v[3]-v[2]=3+2-4=1
    s=1+v[4]-v[3]=1+3-2=2
    s=2+v[5]-v[4]=2+3-3=2  v[5]=v[4] -->n=6
    s=2+v[6]-v[5]=2+10-3=9
    
    n=6 s=9 
    R:B
